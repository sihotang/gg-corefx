{
    "name": "gg/corefx",
    "description": "The GG Core Framework.",
    "keywords": ["framework", "cms", "laravel", "wordpress", "gg"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sopar Sihotang",
            "email": "soparsihotang@gmail.com"
        }
    ],
    "require": {
        "ext-zip": "*",
        "php": ">=5.3.9 | >=7.0.0",
        "guzzlehttp/guzzle": "~4.0|~5.0|~6.0",
        "illuminate/config": "5.5.*",
        "illuminate/container": "5.5.*",
        "illuminate/support": "5.5.*",
        "symfony/console": "~2.3|~3.0",
        "symfony/filesystem": "~2.3|~3.0",
        "symfony/process": "~2.3|~3.0",
        "wp-cli/wp-cli": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7 | ~6.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "GG\\": "src/GG/"
        },
        "files": [
            "src/GG/Support/helpers.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "suggest": {
        "laravel/tinker": "Required to use the tinker console command (~1.0).",
        "wp-cli/wp-cli": "Required to use the wp console command (~1.3).",
        "vlucas/phpdotenv": "Required to use .env files (~2.2)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "bin/artisan",
        "bin/gg"
    ]
}
